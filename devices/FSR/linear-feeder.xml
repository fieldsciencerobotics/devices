<?xml version="1.0"?>
<device>
  <developer>FSR</developer>
  <product>linear-feeder</product>
  <pwrdownmode>false</pwrdownmode>
 
  <regular>  
	<reg name="meat-loaded-thresh" id="11">
      <endpoint name="meat-loaded-thresh" type="num" dir="out">
		<position>0</position>
        <size>2</size>
      </endpoint>
    </reg>
	
    <reg name="meat-dropped-thresh" id="12">
      <endpoint name="meat-dropped-thresh" type="num" dir="out">
		<position>0</position>
        <size>2</size>
      </endpoint>
    </reg>
	
    <reg name="motor-direction" id="13">
      <endpoint name="motor-direction" type="num" dir="out">
		<position>0</position>
        <size>2</size>
      </endpoint>
    </reg>
    
    <reg name="action" id="14">
      <endpoint name="action" type="num" dir="out">
		<position>0</position>
        <size>2</size>
      </endpoint>
    </reg>

	<reg name="action-event" id="15">
      <endpoint name="action-event" type="num" dir="inp">
        <position>0</position>
        <size>2</size>
      </endpoint>
    </reg>
	
	<reg name="battery-voltage" id="16">
      <endpoint name="cell-1" type="num" dir="inp">
        <position>0</position>
        <size>4</size>
      </endpoint>
      <endpoint name="cell-2" type="num" dir="inp">
        <position>4</position>
        <size>4</size>
      </endpoint>
	  <endpoint name="cell-3" type="num" dir="inp">
        <position>8</position>
        <size>4</size>
      </endpoint>
    </reg>
  </regular>  
</device>